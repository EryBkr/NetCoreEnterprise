@model EnterpriseProject.Project.MVCWebUI.Models.ShippingDetailsModel

<h1>Complete</h1>

<form asp-controller="Cart" asp-action="Complete" method="post">
    @Html.AntiForgeryToken()
    <div class="form-group">
        <label asp-for="ShippingDetail.Email">Email address</label>
        <input asp-for="ShippingDetail.Email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
        <span class="text-danger" asp-validation-for="ShippingDetail.Email"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetail.FirstName">FirstName</label>
        <input asp-for="ShippingDetail.FirstName" type="text" class="form-control" >
        <span class="text-danger" asp-validation-for="ShippingDetail.FirstName"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetail.Age">Age</label>
        <input asp-for="ShippingDetail.Age" type="text" class="form-control" >
        <span class="text-danger" asp-validation-for="ShippingDetail.Age"></span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
